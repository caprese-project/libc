#ifndef CAPRESE_LIBC_CXX_UTILITY_
#define CAPRESE_LIBC_CXX_UTILITY_

namespace std {
  [[noreturn]] inline void unreachable() {
#if defined(__GNUC__)
    __builtin_unreachable();
#else  // ^^^ __GNUC__ ^^^ / vvv !__GNUC__ vvv
    static_assert(false, "Unsupported compiler");
#endif // !__GNUC__
  }
} // namespace std

#endif // CAPRESE_LIBC_CXX_UTILITY_
