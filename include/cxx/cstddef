#ifndef CAPRESE_LIBC_CXX_CSTDDEF_
#define CAPRESE_LIBC_CXX_CSTDDEF_

#include <internal/cxx/stddef.h>
#include <stddef.h>

using nullptr_t = std::__nullptr_t;

namespace std {
  using ::max_align_t;
  using ::nullptr_t;
  using ptrdiff_t = __ptrdiff_t;
  using size_t    = __size_t;
} // namespace std

#endif // CAPRESE_LIBC_CXX_CSTDDEF_
